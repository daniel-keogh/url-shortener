{
  "$schema": "https://deno.land/x/denon/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run app.ts",
      "desc": "run my app.ts file",
      "allow": [
        "env",
        "net",
        "plugin",
        "read",
        "write"
      ],
      "env": {
        "PORT": "5000",
        "HOST": "http://localhost:5000",
        "DB_URI": "mongodb://localhost:27017",
        "DB_NAME": "urlShortener"
      },
      "unstable": true
    }
  }
}